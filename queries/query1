PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX stix: <http://stix.mitre.org/STIX#>

SELECT ?indicator
	WHERE { 
    ?indicator 	rdf:type 	stix:Indicator.
		?observable 	rdf:type	stix:Observable.
		?event		rdf:type	stix:Event.
		?indicator	stix:observable ?observable.
		?observable	rdf:type	stix:ScanObservable.	
		}
